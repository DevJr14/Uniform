@page "/getting-started"

<Header OnClose="Cancel" Title="Getting Started" Description="Lets help you get started." />
<MudGrid>
    <MudItem xs="12">
        <MudCard Elevation="25">
            <MudCardContent>
                <MudText>Start by registering your Business or Service with Us.</MudText>
            </MudCardContent>
            <MudCardActions Class="mx-2">
                <MudButton StartIcon="@Icons.Material.Outlined.AddBusiness" OnClick="GetStarted" Variant="Variant.Text" Color="Color.Primary">Click To Start</MudButton>
            </MudCardActions>
        </MudCard>
    </MudItem>
</MudGrid>